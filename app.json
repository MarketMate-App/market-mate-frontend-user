{
  "expo": {
    "name": "MarketMate",
    "slug": "marketmate",
    "description": "MarketMate is a grocery shopping app that allows users to shop for fresh produce with ease and convenience. Its intuitive interface and advanced features make it simple to find and purchase high-quality products.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ico.png",
    "scheme": "com.aceslice.marketmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aceslice.marketmate"
    },
    "android": {
    "usesCleartextTraffic": true,

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ico.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aceslice.marketmate",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCUgJkMiraPAXlkSNeyA3wVFW0amffBkHo"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow MarketMate to access your Face ID biometric data."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ico.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87753e7b-e1f9-4243-a8da-4d4dfe6087fb"
      }
    },
    "owner": "aceslice"
  }
}
